{
  "name": "Gtestrin++",
  "image": "xanonymous/gtest-cpp-essential-env",
  "runArgs": [
    "--cap-add=SYS_PTRACE",
    "--security-opt",
    "seccomp=unconfined"
  ],
  "customizations": {
    "vscode": {
      "settings": {},
      "extensions": [
        "ms-vscode.makefile-tools"
      ]
    }
  },
  "postCreateCommand": "g++ -v",
  "remoteUser": "vscode"
}
